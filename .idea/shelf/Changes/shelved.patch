+import React, { useMemo } from 'react';
+import { MapContainer, GeoJSON, Marker } from 'react-leaflet';
+import L from 'leaflet';
+import * as turf from '@turf/turf';
+import geojsonPrecision from 'geojson-precision';
+import 'leaflet/dist/leaflet.css';
+
+// Фикс для отображения маркеров
+delete (L.Icon.Default.prototype as any)._getIconUrl;
+L.Icon.Default.mergeOptions({
+  iconRetinaUrl: 'https://cdnjs.cloudflare.com/ajax/libs/leaflet/1.7.1/images/marker-icon-2x.png',
+  iconUrl: 'https://cdnjs.cloudflare.com/ajax/libs/leaflet/1.7.1/images/marker-icon.png',
+  shadowUrl: 'https://cdnjs.cloudflare.com/ajax/libs/leaflet/1.7.1/images/marker-shadow.png',
+});
+
+const optimizeGeoJSON = (geojson: any, tolerance: number) => {
+  if (!geojson) return null;
+
+  // Шаг 1: Уменьшение точности координат
+  const rounded = geojsonPrecision.parse(geojson, 4); // 4 знака после запятой
+
+  // Шаг 2: Упрощение геометрии
+  const simplified = turf.simplify(rounded, {
+    tolerance: tolerance,
+    highQuality: true
+  });
+
+  return simplified;
+};
+
+const RussiaMap = ({ regions, rivers, points }) => {
+  const center: [number, number] = [60, 100];
+  const zoom = 3;
+
+  // Оптимизация данных
+  const optimizedRegions = useMemo(() =>
+      optimizeGeoJSON(regions, 0.01), [regions]);
+
+  const optimizedRivers = useMemo(() =>
+      optimizeGeoJSON(rivers, 0.005), [rivers]);
+
+  // Стили
+  const regionStyle = {
+    color: '#2ecc71',
+    weight: 1,
+    fill: false
+  };
+
+  const riverStyle = {
+    color: 'white',
+    weight: 1,
+    opacity: 0.7
+  };
+
+  return (
+      <div style={{ height: '100vh', width: '100%', backgroundColor: '#27ae60' }}>
+        <MapContainer
+            center={center}
+            zoom={zoom}
+            style={{ height: '100%', width: '100%' }}
+            minZoom={2}
+            maxBounds={[[40, 30], [80, 190]]}
+            attributionControl={false}
+        >
+          {optimizedRegions && (
+              <GeoJSON
+                  data={optimizedRegions}
+                  style={regionStyle}
+              />
+          )}
+
+          {optimizedRivers && (
+              <GeoJSON
+                  data={optimizedRivers}
+                  style={riverStyle}
+              />
+          )}
+
+          {points.map((point, index) => (
+              <Marker
+                  key={`point-${index}`}
+                  position={point.position}
+                  title={point.name}
+              />
+          ))}
+        </MapContainer>
+      </div>
+  );
+};
